{
  "manifest_version": 2,

  "name": "Fix LinkedIN Plugin",
  "description": "This extension will remove the news bar. More to come!",
  "version": "0.9",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "donate.html"
  },
  "background": {
	"scripts": ["background.js"]
  },  
  "permissions": [
   "activeTab"
   ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
    }
  ]
}